<%- include("partials/header") %>
<div class="container">
    <div class="row">
        
        <div style="width: 30%; margin: 30px auto;">
            <h1 style="text-align: center;">Signup</h1>
            <form  action="/register" method="post">
                <div class="form-group"><input class="form-control" type="email" name="username" placeholder="enter email id"></div>
                <div class="form-group"><input class="form-control" type="text" name="user" placeholder="enter name"></div>
                <div class="form-group"><input class="form-control" type="password" name="password" placeholder="Enter password"></div>
                <div class="form-group"><textarea name="address" cols="40" rows="10" placeholder="enter your address"></textarea></div>
                <div class="form-group"><button class="btn btn-lg btn-primary btn-block" type="submit">submit</button></div>
                
            </form>
        </div>

    
    </div>
</div>

    <%- include("partials/footer") %>